{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "admin",
      "password": "admin123",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "admin",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "scraping_exchange",
      "vhost": "/",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "queues": [
    {
      "name": "scraping_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-max-priority": 10
      }
    },
    {
      "name": "scraping_results",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 1000
      }
    },
    {
      "name": "scraping_failed",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-message-ttl": 604800000,
        "x-max-length": 1000
      }
    }
  ],
  "bindings": [
    {
      "source": "scraping_exchange",
      "vhost": "/",
      "destination": "scraping_queue",
      "destination_type": "queue",
      "routing_key": "scraping",
      "arguments": {}
    },
    {
      "source": "scraping_exchange",
      "vhost": "/",
      "destination": "scraping_results",
      "destination_type": "queue",
      "routing_key": "result",
      "arguments": {}
    },
    {
      "source": "scraping_exchange",
      "vhost": "/",
      "destination": "scraping_failed",
      "destination_type": "queue",
      "routing_key": "failed",
      "arguments": {}
    }
  ]
}
